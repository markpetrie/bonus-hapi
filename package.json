{
  "name": "basic-mongoose",
  "version": "1.0.0",
  "description": "Full CRUD for a validated resource using ExpressJS and Mongoose",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "scripts": {
    "test": "mocha tests --recursive",
    "dev": "nodemon --exec 'npm run test -- -w' ",
    "test:unit": "mocha tests/unit",
    "dev:unit": "nodemon --exec 'npm run test/unit'",
    "test:e2e": "mocha tests/e2e",
    "dev:e2d": "nodemon --exec 'npm run test/e2e'",
    "start": "nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markpetrie/basic-mongoose.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/markpetrie/basic-mongoose/issues"
  },
  "homepage": "https://github.com/markpetrie/basic-mongoose#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "eslint": "^4.3.0",
    "express": "^4.15.3",
    "mocha": "^3.4.2",
    "mongoose": "^4.11.4",
    "morgan": "^1.8.2",
    "nodemon": "^1.11.0"
  }
}
